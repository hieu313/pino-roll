{
  "name": "pino-roll",
  "version": "3.1.0",
  "description": "A Pino transport that automatically rolls your log files (forked from mcollina/pino-roll to use dayjs instead of date-fns)",
  "main": "pino-roll.js",
  "scripts": {
    "dev": "tap -w -R spec",
    "prepare": "husky install",
    "test": "standard | snazzy && tap",
    "test-ci": "standard | snazzy && tap --coverage-report=lcovonly"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hieu313/pino-roll.git"
  },
  "keywords": [
    "pino",
    "transport"
  ],
  "author": "Damien Simonin Feugas <damien.feugas@gmail.com>",
  "contributors": [
    "Matteo Collina <hello@matteocollina.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "dayjs": "^1.11.10",
    "sonic-boom": "^4.0.1"
  },
  "devDependencies": {
    "husky": "^9.0.11",
    "mockdate": "^3.0.5",
    "pino": "^9.2.0",
    "snazzy": "^9.0.0",
    "standard": "^17.0.0-2",
    "tap": "^16.0.1"
  }
}
